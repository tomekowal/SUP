{% extends "devices/devices.dtl" %}

{% block head %}
<script src="/javascripts/devices.js"></script>
{% endblock %}

{% block yield %}
    <form action="/devices" method="post" onsubmit="return validate()">
      <table class="devices">
        {% for field in device_fields %}
          <tr>
            <td>{{ field }}</td>
            <td><input type="text" name="{{ field }}"></td>
          </tr>
        {% endfor %}
      </table>
      <input type="submit">
    </form>
{% endblock %}
